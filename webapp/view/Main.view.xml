 <mvc:View controllerName="p36.snacktime.controller.Main"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
  <App id="idAppControl">
    <pages>
      <Page>
        <headerContent>
          <Bar>
            <contentLeft>
              <Image class="logo" src="../img/logo.svg"/>
            </contentLeft>
          </Bar>
        </headerContent>
        <content>
          <FlexBox items="{order>/suppliers}">
              <items>
                  <GenericTile class="sapUiSmallMargin" header="{order>title}" press="onPressSupplier"/>
              </items>
          </FlexBox>
        </content>
      </Page>
    </pages>
  </App>
</mvc:View>