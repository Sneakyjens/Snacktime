{
  "_version": "1.12.0",
  "sap.app": {
    "id": "p36.snacktime",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "rootView": {
      "viewName": "p36.snacktime.view.FlexibleColumnLayout",
      "type": "XML",
      "async": true,
      "id": "fcl"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "p36.snacktime.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "order": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "p36.snacktime.view",
        "controlId": "fcl",
        "transition": "slide",
        "async": true
      },
      "routes": [
        {
          "name": "Home",
          "pattern": "",
          "target": ["TargetHome"]
        },
        {
          "name": "OrderDetail",
          "pattern": "Order/{orderpath}",
          "target": ["TargetHome", "TargetOrderDetail"]
        }
      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "controlAggregation": "beginColumnPages",
          "viewName": "Main"
        },
        "TargetOrderDetail": {
          "viewType": "XML",
          "controlAggregation": "midColumnPages",
          "viewName": "OrderDetail"
        }
      }
    }
  }
}
